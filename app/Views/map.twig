        var uluru3 = {lat: 32.064742, lng: 31.249509};
        // The map, centered at Uluru
        var map = new google.maps.Map(
                document.getElementById('map'), {zoom: 7, center: uluru});
        var map = new google.maps.Map(
                document.getElementById('map'), {zoom: 7, center: uluru2});
        var map = new google.maps.Map(
                document.getElementById('map'), {zoom: 7, center: uluru3});
        // The marker, positioned at Uluru
        var marker = new google.maps.Marker({position: uluru, map: map});
        var marker = new google.maps.Marker({position: uluru2, map: map});
        var marker = new google.maps.Marker({position: uluru3, map: map});
